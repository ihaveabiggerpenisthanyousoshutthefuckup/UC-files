        <text>
            <![CDATA[<b>F0/1</b><br/>Root ID = 24577.000A00333333<br/>Bridge ID = 24577.000A00333333<br/>Path Cost = 19]]></text>
    </component>
    <component id="ID_s0_txt08">
        <text>
            <![CDATA[<b>F0/2</b><br/>Root ID = 24577.000A00333333<br/>Bridge ID = 24577.000A00333333<br/>Path Cost = 19]]></text>
