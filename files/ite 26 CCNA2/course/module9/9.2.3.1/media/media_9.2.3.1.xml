	<component id="ID_title">
		<!--text><![CDATA[<b>Configuring PAT using an Address Pool</b>]]></text-->
	</component>
	<component id="ID_s3_SC5" fontSize="12" fontFace="_typewriter" fontColor="#ffffff">
		<feedback id="fb"><![CDATA[<b><font face="_sans">***You must enter the exact and full command. Commands that include a name or password are case-sensitive.***</font></b>
		]]></feedback>
		<existingcommands><![CDATA[<font face="_sans"><b>Define a pool of public IPv4 addresses 209.165.200.241 to 209.165.200.250 with pool name NAT-POOL-OVERLOAD.</b></font>]]></existingcommands>
		<command prompt="R2(config)#">
			<correctresponse ignoreCase="false"><![CDATA[ip nat pool NAT-POOL-OVERLOAD 209.165.200.241 209.165.200.250 netmask 255.255.255.224]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>Configure ACL 3 to permit devices from 10.0.0.0/8 network to be translated by NAT.</font>]]></instruction>
		</command>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[access-list 3 permit 10.0.0.0 0.255.255.255]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>Bind NAT-POOL-OVERLOAD with ACL 3.</font>]]></instruction>
		</command>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[ip nat inside source list 3 pool NAT-POOL-OVERLOAD overload]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>Configure the proper inside NAT interface.</font>]]></instruction>
		</command>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[interface Serial0/0/0]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[ip nat inside]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>Configure the proper outside NAT interface.</b></font>]]></instruction>
		</command>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[interface Serial0/1/0]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[ip nat outside]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>You successfully configured PAT using an Address Pool.</font>]]></instruction>
		</command>
		<command prompt=""/>
	</component>
